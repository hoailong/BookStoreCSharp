                                {
                                    <tr id="@book.BookId">
                                        <td>1</td>
                                        <td class="book-id">@book.BookId</td>
                                        <td class="book-name">@book.BookId</td>
                                        <td class="book-quant">@book.Count</td>
                                    </tr>

                                }*@

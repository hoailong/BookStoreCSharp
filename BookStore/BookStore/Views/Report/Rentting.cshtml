@model IEnumerable<BookRent>
@{
    ViewData["Title"] = "Danh sách sách truyện đang thuê ";
}

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">

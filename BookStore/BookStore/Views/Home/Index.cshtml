                        </ul>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">

                    <table class="table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>ID</th>
                                <th>Name</th>
                                <th>Old</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>1</td>
                                <td>1000</td>
                                <td>Otto</td>
                                <td>19</td>
                            </tr>
                            <tr>
                                <td>2</td>
                                <td>1001</td>
                                <td>Thornton</td>
                                <td>22</td>
                            </tr>
                            <tr>
                                <td>3</td>
                                <td>1001</td>
                                <td>the Bird</td>
                                <td>15</td>
                            </tr>
                            @foreach (var test in Model)
                            {
                                <tr>
                                    <td>4</td>
                                    <td>@test.id</td>
                                    <td>@test.name</td>
                                    <td>@test.old</td>
                                </tr>

                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>
